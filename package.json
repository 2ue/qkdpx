{
  "name": "qkdpx",
  "version": "0.0.4",
  "description": "A fast and modern CLI tool for npm package publishing with automated workflow",
  "homepage": "https://github.com/2ue/qkdpx#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/2ue/qkdpx.git"
  },
  "bugs": {
    "url": "https://github.com/2ue/qkdpx/issues"
  },
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "qkdpx": "bin/qkdpx.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "npm",
    "publish",
    "cli",
    "automation",
    "versioning",
    "git",
    "workflow",
    "typescript",
    "semantic-versioning",
    "package-management"
  ],
  "author": "2ue <https://github.com/2ue>",
  "license": "MIT",
  "files": [
    "dist",
    "bin",
    "README.md",
    "README.zh-CN.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.2.0",
    "listr2": "^6.6.1",
    "ora": "^7.0.1",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^20.5.0",
    "@types/semver": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.2",
    "rimraf": "^5.0.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6"
  }
}
